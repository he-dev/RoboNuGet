 </ItemGroup>

 <ItemGroup>
   <Reference Include="System.ComponentModel.DataAnnotations" />
 </ItemGroup>

 <ItemGroup>
   <Compile Include="..\..\Reusable\Reusable.Commander\src\Commands\Help.cs">
     <Link>src\Linked\Help.cs</Link>
